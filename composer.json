{
    "name": "spoonx/sxbootstrap",
    "description": "A twitter bootstrap module for ZF2 (zend framework 2) based on rwoverdijk/assetmanager",
    "license": "BSD-2-Clause",
    "keywords": [
        "twitter",
        "bootstrap",
        "assetmanager",
        "zf2",
        "zend framework"
    ],
    "homepage": "https://github.com/SpoonX/SxBootstrap",
    "require": {
        "php": ">=5.3.3",
        "rwoverdijk/assetmanager": "1.*",
        "twitter/bootstrap": "2.*"
    },
    "replace": {
        "rwoverdijk/sxbootstrap": "*"
    },
    "authors": [
        {
            "name": "Wesley Overdijk",
            "email": "r.w.overdijk@gmail.com",
            "homepage": "http://blog.spoonx.nl/"
        }
    ],
    "autoload" : {
        "psr-0": {
            "SxBootstrap": "src/"
        },
        "classmap": [
            "./"
        ]
    }
}
